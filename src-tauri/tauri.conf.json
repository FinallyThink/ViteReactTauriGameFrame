{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "wstg",
  "version": "0.1.0",
  "identifier": "com.kaiqiao.wstg",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5910",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "wstg",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/**/*"
    ]
  }
}