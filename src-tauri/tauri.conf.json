{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "project_1",
  "version": "0.1.0",
  "identifier": "qkProject_1",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5174/",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "my-tauri-app",
        "width": 800,
        "height": 600,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico"]
  }
}
